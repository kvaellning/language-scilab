
'.source.scilab':
  'editor':
    'autoIndentOnPaste': true
    'softTabs': true
    'tabLength': 3
    'commentStart': '// '
    'increaseIndentPattern': '^\\s*(function|if|else|elseif|select|case|try|catch|for|while)\\b.*(?<!end)\\s*$'
    'decreaseIndentPattern': '^\\s*(case|catch|elseif|else|end|endfunction)\\b.*\\s*$'
